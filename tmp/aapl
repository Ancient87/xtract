<script language='javascript'>
    function getObj(name) {
			if (document.getElementById) this.obj = document.getElementById(name);
			else if (document.all) this.obj = document.all[name];
			else if (document.layers) this.obj = document.layers[name];
			if (this.obj) this.style = this.obj.style;
		}
  var object_togo = new getObj('OAS_x61');
  var object_tomove = new getObj('Hidden_OAS_x61');
  if (object_togo.obj!=null && object_tomove.obj!=null) 
	{ 
	  object_togo.obj.appendChild(object_tomove.obj);
	  object_tomove.style.display = "inline";
	}
  /* if (document.getElementById) {
     var x61Obje= document.getElementById("OAS_x61");
     if(x61Obje!=null){
        var jsE = document.createElement("script");
        jsE.setAttribute("type", "text/javascript");
        jsE.setAttribute("value", "OAS_AD('tickertapebutton');");
        x61Obje.appendChild(jsE);
     }
   }*/
   
