function i18_data(jquery_path,config){
	if(null==config){
		config={format:"#,###.0",locale:"us"};
	}
	
	$(jquery_path).each(
		function(){$(this).format(config);}
	);
}

var winPopups = new Array();
window.onunload = function () {
	for (var i = 0; i < winPopups.length; i++) {
		winPopups[i].close();
	}
};
function openWin(url, windowName, windowFeatures) {
	if (windowName == "")
		windowName = "newWindow";
	if (windowFeatures == "")
		windowFeatures = "width=750,height=600,top=30,left=30,toolbar=yes,menubar=yes,scrollbars=yes,resizable=yes"
	var win = window.open(url, windowName, windowFeatures);
	if (win != null) {
		winPopups.push(win);
		win.focus();
	}
}
var sAds ="";
sAds = "&adsFlag=true";

function loadCurrentValuation(){
	var urlstr = "//financials.morningstar.com/valuate/current-valuation-list.action?&t=XNAS:AAPL&region=usa&culture=en-US&cur="+sAds;
	$.ajaxSetup({cache:false});
	$.get(
		urlstr,
		{},
		function(data){
			$("#currentValuationList").html(data);
		});
}

function loadForwardValuation(){
	var urlstr = "//financials.morningstar.com/valuate/forward-valuation-list.action?&t=XNAS:AAPL&region=usa&culture=en-US&cur="+sAds;
	$.ajaxSetup({cache:false});
	$.get(
		urlstr,
		{},
		function(data){
			$("#forwardValuationList").html(data);
		});
}

function locateTo(url){
	window.location.href=url+"";
}

function loadValuationHistory(historyType){
	if(null==historyType) historyType="";

	$.ajaxSetup({cache:false});
	$.get(
		"//financials.morningstar.com/valuate/valuation-history.action?&t=XNAS:AAPL&region=usa&culture=en-US&cur=&type="+historyType,
		{},
		function(data){
			$("#valuationHistory").html(data);
		});
}

function loadValuationYield(){
	$.ajaxSetup({cache:false});
	$.get(
		"//financials.morningstar.com/valuate/valuation-yield.action?&t=XNAS:AAPL&region=usa&culture=en-US&cur=",
		{},
		function(data){
			$("#valuationYield").html(data);
		});
}
