<script language="javascript">
$(window).unload(function(){
	var ScrollY = 0;//window.pageYOffset || document.documentElement.scrollTop || 0;
	document.cookie = "ScrollY=" + ScrollY + ";path=/;domain=.morningstar.com;";
});

//Read and scroll
function ReadCookieAndScroll(){
	var ScrollY = GetCookie("ScrollY");
    if (ScrollY !== "" && !isNaN(ScrollY))
    	window.scrollTo(0, ScrollY); 
}

function GetCookie(name){
	if(document.cookie.length<=0)
    	return "";

   	name += "=";
    var start = document.cookie.indexOf(name);
    if (start < 0)
       return "";

	start += name.length;
    var end = document.cookie.indexOf(";",start);
    if (end < 0)
    	end = document.cookie.length;
              
   	return document.cookie.substring(start,end);
}

//var bloomreachConfig = {
//"category" : "Valuation",
//"productName" : "AAPL"
//};
//setBoolemreachConfig(bloomreachConfig);

</script>
<script language="javascript">
function turnon(b) {
for ( var a = 1; a <= 12; a++) {
	//document.getElementById("Tabmenu" + a).className = "sdir_menu";
	document.getElementById("menu" + a).style.display = "none"
}
	document.getElementById("Tab" + b).className = "sdir_menuOn";
	document.getElementById(b).style.display = "block"
} 
</script>
