<script class="script-for-jprint" language="javascript">
	function initPageCtx(){
		//chart history 
		optionHistory.yaxis.labelWidth = 125;
		pageCtx.optionHistory=optionHistory;
		pageCtx.dataOffsetHistory=dataOffsetHistory;
		pageCtx.dataHistory=dataHistory;
		
		//chart Yield
		optionYield.yaxis.labelWidth = 125;
		pageCtx.optionYield=optionYield;
		pageCtx.dataYield=dataYield;
	}
	
	function drawHistory4Print(canvasID,size) {
		var canvasObj = {ctx:null,octx:null,eventHolder:null,size:size};
		createCanvas($("#"+canvasID),canvasObj);
        flotObj = $.plot(canvasID, pageCtx.optionHistory, canvasObj);
        flotObj.init(pageCtx.dataHistory, pageCtx.optionHistory, null, null, pageCtx.dataOffsetHistory);
    }
    
	function drawYield4Print(canvasID,size) {
		var canvasObj = {ctx:null,octx:null,eventHolder:null,size:size};
		createCanvas($("#"+canvasID),canvasObj);
        var flotObj = $.plot(canvasID, pageCtx.optionYield, canvasObj);
        flotObj.init(pageCtx.dataYield, pageCtx.optionYield);
    }
    
	function callback4jPrint(){
		
		$("#barChart").html("");
		$.zoomContainer("barChart", 955, 189, true);
		drawHistory4Print("barChart", [630, 191]);
		
		$("#yeldChart").html("");
		$.zoomContainer("yeldChart", 955, 189, true);
		drawYield4Print("yeldChart", [630, 191]);
	}
</script>


